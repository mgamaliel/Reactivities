{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "sourceMap": true,
        "target": "ES5"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
